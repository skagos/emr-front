Get-ChildItem -Path . -Filter '*.gz' -File | ForEach-Object {
    Write-Host "Extracting $($_.Name)..."
    $outFile = [IO.Path]::ChangeExtension($_.FullName, $null)
    $inStream = [IO.File]::OpenRead($_.FullName)
    $outStream = [IO.File]::Create($outFile)
    $gzipStream = New-Object IO.Compression.GzipStream($inStream, [IO.Compression.CompressionMode]::Decompress)
    $gzipStream.CopyTo($outStream)
    $gzipStream.Close()
    $outStream.Close()
    $inStream.Close()
}